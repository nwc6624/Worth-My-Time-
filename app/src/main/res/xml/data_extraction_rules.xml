<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude database files from cloud backup -->
        <exclude domain="database" path="worth_my_time.db"/>
        <exclude domain="database" path="worth_my_time.db-shm"/>
        <exclude domain="database" path="worth_my_time.db-wal"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude database files from device transfer -->
        <exclude domain="database" path="worth_my_time.db"/>
        <exclude domain="database" path="worth_my_time.db-shm"/>
        <exclude domain="database" path="worth_my_time.db-wal"/>
    </device-transfer>
</data-extraction-rules>
